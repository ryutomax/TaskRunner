    <div class="l-wrap-layer">
      <!-- 閉じタグは_footer.ejs -->
      <!-- 画像相対パス(dist参照) -->
      <% const mediaPath=["../assets/images/"]%>
        <%- include("../include/_header", {path: '../' }) -%>
          <main class="l-main js-scroll-mask">
            <button class="c-toUpBtn js-toUpBtn u-zindex_1">
            </button>
            <section class="p-video u-zindex_-10">
              <video class="p-video-inner" poster="<%= mediaPath %>common/top_mv_img.jpg" webkit-playsinline playsinline muted autoplay loop>
